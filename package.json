{
  "name": "marketplace-b2b-api",
  "version": "1.0.0",
  "description": "API REST para um **Marketplace B2B de Serviços Profissionais**, onde empresas contratantes podem publicar demandas e prestadores de serviços enviam propostas, firmam contratos, recebem pagamentos e avaliações.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "format": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusz17/marketeplace-b2b-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matheusz17/marketeplace-b2b-api/issues"
  },
  "homepage": "https://github.com/matheusz17/marketeplace-b2b-api#readme",
  "dependencies": {
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
